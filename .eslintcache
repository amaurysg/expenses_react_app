>\r\n    \r\n    </>\r\n  )\r\n}\r\n\r\nexport default DatePicker\r\n","D:\\Documentos\\app_react_end\\expenses_react_app\\src\\firebase\\AddGasto.js",[],"D:\\Documentos\\app_react_end\\expenses_react_app\\src\\components\\BarraTotal.js",[],"D:\\Documentos\\app_react_end\\expenses_react_app\\src\\funciones\\ConvertToCoin.js",[],"D:\\Documentos\\app_react_end\\expenses_react_app\\src\\hooks\\useObtenerGastos.js",["139"],"D:\\Documentos\\app_react_end\\expenses_react_app\\src\\elements\\ElementosListas.js",[],"D:\\Documentos\\app_react_end\\expenses_react_app\\src\\firebase\\BorrarGasto.js",[],{"ruleId":"140","severity":1,"message":"141","line":5,"column":8,"nodeType":"142","messageId":"143","endLine":5,"endColumn":15},{"ruleId":"140","severity":1,"message":"144","line":16,"column":9,"nodeType":"142","messageId":"143","endLine":16,"endColumn":20},{"ruleId":"145","replacedBy":"146"},{"ruleId":"147","replacedBy":"148"},{"ruleId":"140","severity":1,"message":"149","line":3,"column":43,"nodeType":"142","messageId":"143","endLine":3,"endColumn":60},{"ruleId":"140","severity":1,"message":"150","line":4,"column":9,"nodeType":"142","messageId":"143","endLine":4,"endColumn":26},{"ruleId":"140","severity":1,"message":"151","line":4,"column":47,"nodeType":"142","messageId":"143","endLine":4,"endColumn":58},{"ruleId":"140","severity":1,"message":"152","line":11,"column":8,"nodeType":"142","messageId":"143","endLine":11,"endColumn":14},{"ruleId":"140","severity":1,"message":"153","line":10,"column":15,"nodeType":"142","messageId":"143","endLine":10,"endColumn":17},{"ruleId":"140","severity":1,"message":"154","line":16,"column":5,"nodeType":"142","messageId":"143","endLine":16,"endColumn":22},{"ruleId":"140","severity":1,"message":"155","line":17,"column":5,"nodeType":"142","messageId":"143","endLine":17,"endColumn":28},{"ruleId":"140","severity":1,"message":"156","line":1,"column":8,"nodeType":"142","messageId":"143","endLine":1,"endColumn":13},{"ruleId":"140","severity":1,"messag